      "backup": 7
    },
    "captcha": {
      "enable": false,
      "provider": null,
      "hosts": {}
    }
  },
  "manager": {
    "enable": true,
    "logger": {
      "enable": true,
      "name": "iklab_manager",
      "level": "ERROR",
      "backup": 7
    }
  },
  "database": {
    "enable": true,
    "engine": "SQLITE3",
    "name": "iklab"
  }
}